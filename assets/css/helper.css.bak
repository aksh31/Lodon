*
{
	padding:0;
	margin:0;
	box-sizing:border-box;
}
.row, .row:after, .row:before
{
	clear:both;
	display:table;
	width:100%;
}
a:link
{
	text-decoration:none;
}
.container
{
	width:1145px;
	margin:0 auto;
}
.col-2, .col-3, .col-5, .col-6, .col-7, .col-8
{
	float:left;
	padding:0 15px;
}
.row > :first-child
{
	padding-left:0;
}
.row > :last-child
{
	padding-right:0;
}
.col-2
{
	width:calc((100% * 2) / 12);
}
.col-3
{
	width:calc((100% * 3) / 12);
}
.col-5
{
	width:calc((100% * 5) / 12);
}
.col-6
{
	width:calc((100% * 6) / 12);
}
.col-7
{
	width:calc((100% * 7) / 12);
}
.col-8
{
	width:calc((100% * 8) / 12);
}
ul
{
	list-style:none;
}
.text-center
{
	text-align:center;
}
.text-right
{
	text-align:right;
}
.text-up
{
	text-transform:uppercase;
}
.text-cap
{
	text-transform:capitalize;
}
.pr-25
{
	padding-right:25px;
}
.pl-40
{
	padding-left:40px;
}
.pr-40
{
	padding-right:40px;
}
.pl-55
{
	padding-left:55px;
}
.pr-55
{
	padding-right:55px;
}
.mb-70
{
	margin-bottom:70px;
}
.mb-50
{
	margin-bottom:50px;
}
.mb-25
{
	margin-bottom:25px;
}
.mb-21
{
	margin-bottom:21px;
}
.mb-18
{
	margin-bottom:18px;
}
.mb-13
{
	margin-bottom:18px;
}
.mb-8
{
	margin-bottom:8px;
}
.mt-25
{
	margin-top:25px;
}
.mt-15
{
	margin-top:15px;
}
.w-9
{
	width:calc((100% * 9) / 12);
	margin:0 auto;
}